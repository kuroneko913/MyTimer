{
    "name": "MyUtility",
    "version": "1.0",
    "description": "作業しすぎ防止とルーチンワークを忘れないようにするための機能",
    "permissions": ["tabs", "declarativeContent", "storage", "contextMenus", "alarms"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "manifest_version": 2
}